#include<iostream>
#include<conio.h>
#include<string.h>
using namespace std;

struct regvecpalabras
{
	char vecp[15];
};
struct regvector
{
    regvecpalabras vec[10];
};

void cargarvec(regvecpalabras vec[],int n)
{
int i;
for(i=0;i<n;i++)
{
cout<<"ingrese la palabra:";
gets(vec[i].vecp);
}
}
void mostrarvec(regvecpalabras vec[],int n)
{
int i;
for(i=0;i<n;i++)
{
cout<<vec[i].vecp<<" " ;
}
}
