void palabras(regvecpalabras vec[],int np,regvector vec2[],int nv)
{
int i,j,;

for(i=0;i<np;i++)
{
   for(j=0;j<nv;j++)
   {
	   
	   if(i<j)
		   {
			   vec[i].vec2[j]
	       }
	   if(i>j)
	   {
	   vec[]=vec2[];
	   }
	   if(i==j)
	   {
	   vec[i]=vec2[j];
	   }
   }
}
}
void main()
{
	int n;
	regvecpalabras vec[10];
cout<<"ingrese la cantidad de elementos del vector palabras :";
cin>>n;
cargarvec(vec,n);
mostrarvec(vec,n);

getch();
}
