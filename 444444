// intfor.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"


int main()
{
	int n,c1,c2,c3,c4,c5,prom,mat;
	printf ("ingresar la cantidad de estudiantes: \n");
	scanf("%d",&n);
	for (int i=1; i<=n ; i++)
	{
		printf ("ingresar la matricula: \n");
	    scanf("%d",&mat);
		printf("ingresar las 5 calificaciones : \n");
		scanf("%d%d%d%d%d",&c1,&c2,&c3,&c4,&c5);
		prom=(c1+c2+c3+c4+c5)/5;
		printf("la matricula es : %d \n",mat);
	    printf("el promedio es : %d \n",prom);
	}
	return 0;
}
