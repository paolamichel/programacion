// vec.cpp: define el punto de entrada de la aplicaci√≥n de consola.
//

#include "stdafx.h"

void cargar(int v[],int n)
{
int x;
for(int i=0;i<n;i++)
{
	printf("\n elemento :");
	scanf("%d",&x);
	v[i]=x;
}
}

void mostrar(int v[],int n)
{
for(int i=0;i<n;i++)
{
printf("\n %d", v[i]);
}
printf("\n");
}
int main()
{
	int v1[50],n=3;
    int v2[50];
    int v3[50];
	cargar(v1,n);
	cargar(v2,n);
	cargar(v3,n);
	mostrar(v1,n);mostrar(v2,n);mostrar(v3,n);
	return 0;
}
