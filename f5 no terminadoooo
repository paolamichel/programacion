/ funcionesss.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include "math.h"

float distancia(float x1,float y1,float x2,float y2 )
{
    float d;
	d=sqrt(pow(x2-x1,2)+ pow(y2-y1,2));
	return(d);
}

int main()
{
	float x1,y1,x2,y2;
	float D;
	printf("ingresar el valor de x1 :");
	scanf("%f",&x1);
    printf("ingresar el valor de y1 :");
	scanf("%f",&y1);
    printf("ingresar el valor de x2 :");
	scanf("%f",&x2);
    printf("ingresar el valor de y2 :");
	scanf("%f",&y2);
	D=distancia(x1,y1,x2,y2);
	printf("la distancia entre los dos puntos es: %f",D);
	return 0;
}
