// funcioneis.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"

int mayor(int x1,int x2,int x3)
{
if(x1>x2 && x1>x3)
 {
 return x1;
 }
if(x2>x1 && x2>x3)
 {
 return x2;
 }
if(x3>x1 && x3>x2)
 {
 return x3;
 }
}
int main()
{
	int x1,x2,x3,m;
	printf("ingrese el 1er numero");
	scanf("%d",&x1);
	printf("ingrese el 2do numero");
	scanf("%d",&x2);
    printf("ingrese el 3er numero");
	scanf("%d",&x3);
	m=mayor(x1,x2,x3);
	printf("el mayor es :%d \n ",m);

	return 0;
}

