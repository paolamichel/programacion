// vectores2.cpp: define el punto de entrada de la aplicaci√≥n de consola.
#include "stdafx.h"

void cargar(int v[],int n)
{
int x;
for(int i=0;i<n;i++)
{
	printf("\n elemento :");
	scanf("%d",&x);
	v[i]=x;
}
}
void mostrar(int v[],int n)
{
for(int i=0;i<n;i++)
{
printf("%d", v[i]);
}
}
void eliminarpri(int v[],int n)
{
	for(int i=1;i<n;i++)
    {
     printf("\n %d",v[i]);
	}
}
int main()
{
	int v[50],n,x;
	printf(" \n ingresar la dimension del vector:");
	scanf("%d",&n);
	cargar(v,n);
	mostrar(v,n);
	eliminarpri(v,n);
	
	return 0;
}
