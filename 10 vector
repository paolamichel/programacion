// vectores2.cpp: define el punto de entrada de la aplicaci√≥n de consola.
#include "stdafx.h"

void fibonacci(int v[],int n)
{
    int	a=-1;
    int b=1;
	int c=a+b;
	for(int i=0;i<n;i++)
    {
     a=b;
	 b=c;
	 c=a+b;
	 v[i]=c;
	printf(" %d", v[i]);
	}
}
int main()
{
	int v[50],n;
	printf(" \n ingresar la dimension del vector:");
	scanf("%d",&n);
	fibonacci(v,n);
	
	return 0;
}
