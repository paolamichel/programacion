// nuevoprograma.cpp: define el punto de entrada de la aplicaci√≥n de consola.
//

#include "stdafx.h"


int main()
{
	int n;
	float s=0;
	float i=1;
	printf("ingresar la cantidad de numeros que quiere sumar : ");
	scanf("%d",&n);
	while (i<=n)
	{
		s = s+ (i*10+2);
		i=i+1;
	}
    printf("la suma es %f /n : ",s);

	return 0;
}
