// vectores2.cpp: define el punto de entrada de la aplicaci√≥n de consola.
#include "stdafx.h"

void cargar(int v[],int n)
{
int x;
for(int i=0;i<n;i++)
{
	printf("\n elemento :");
	scanf("%d",&x);
	v[i]=x;
}
}
void mostrar(int v[],int n)
{
for(int i=0;i<n;i++)
{
printf("%d", v[i]);
}
}
void eliminarpares(int v[],int n)
{
for(int i=0;i<n;i++)
 {
  if (v[i]%2==1)
  {
	  printf("%d",v[i]);
  }
   
 }
}
int main()
{
	int v[50],n;
	printf(" \n ingresar la dimension del vector:");
	scanf("%d",&n);
	cargar(v,n);
	mostrar(v,n);
	eliminarpares(v,n);
	return 0;
}
